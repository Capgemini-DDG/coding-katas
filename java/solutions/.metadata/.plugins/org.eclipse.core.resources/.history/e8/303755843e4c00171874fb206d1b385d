package com.df.Program;

public class Greed 
{
	public int score(Integer[] input)
	{
		Integer[] count = new Integer[6];
		
		for (int temp : input)
		{
			if (temp == 1)
				count[0] =+ 1;
			else if (temp == 2)
				count[1] =+ 1;
			else if (temp == 3)
				count[2] =+ 1;
			else if (temp == 4)
				count[3] =+ 1;
			else if (temp == 5)
				count[4] =+ 1;
			else if (temp == 6)
				count[5] =+ 1;
		}
		return 0;
	}
}
