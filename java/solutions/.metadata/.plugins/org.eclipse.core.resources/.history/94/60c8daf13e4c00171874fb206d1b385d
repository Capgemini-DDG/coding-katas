package com.df.Program;

public class Greed 
{
	public int score(Integer[] input)
	{
		Integer[] count = {0,0,0,0,0,0};
		
		for (int temp : input)
		{
			if (temp == 1)
				count[0] =+ 1;
			else if (temp == 2)
				count[1] =+ 1;
			else if (temp == 3)
				count[2] =+ 1;
			else if (temp == 4)
				count[3] =+ 1;
			else if (temp == 5)
				count[4] =+ 1;
			else if (temp == 6)
				count[5] =+ 1;
			else
			{}
		}
		
		if (count[0] == 1)
			return 100;
		if (count[4] == 1)
			return 50;
		else
			return 0;
	}
}
