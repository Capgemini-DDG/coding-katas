public class Test1 
{
	FizzBuzz fb = null;
	
	@Before
	public void setUp()
	{
		fb = new FizzBuzz();
	}
	
	@After
	public void tearDown()
	{
		fb = null;
	}
	
	@Test
	public void test1() 
	{
		assertEquals("1", fb.inFizz(1));
	}
}
